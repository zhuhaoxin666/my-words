<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="17dp"
        app:layout_constraintEnd_toStartOf="@+id/listword"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/star_big_on" />

    <TextView
        android:id="@+id/listword"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="sans-serif-black"
        android:text="TextView"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/listmeaning"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/listmeaning"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:layout_marginEnd="43dp"
        android:fontFamily="sans-serif-condensed"
        android:text="TextView"
        app:layout_constraintBaseline_toBaselineOf="@+id/listword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/listword" />
</androidx.constraintlayout.widget.ConstraintLayout>